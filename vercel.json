{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "env": {
    "GOOGLE_CLOUD_STORAGE_BUCKET": "@GOOGLE_CLOUD_STORAGE_BUCKET",
    "GOOGLE_CLOUD_VISION_KEY_BASE64": "@GOOGLE_CLOUD_VISION_KEY_BASE64"
  }
}