{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "api/index.py" }
  ],
  "env": {
    "GOOGLE_CLOUD_STORAGE_BUCKET": "your-gcs-bucket-name",
    "GOOGLE_CLOUD_VISION_KEY_BASE64": "@google_cloud_vision_key_base64"
  }
}